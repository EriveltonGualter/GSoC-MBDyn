	max iterations: 10;
	tolerance: 1.e-6;
end: initial value;

begin: control data;
	structural nodes: 1;
	rigid bodies: 1;
	loadable elements: 1;
end: control data;

begin: nodes;
	structural: 1, dynamic,
		null,
		eye,
		1., 0., 0.,
		null;
end: nodes;

begin: elements;
	set: const real M = 1;
	set: const real K = (2*pi)^2;
	set: const real C = .1*2*sqrt(K*M);

	set: const integer N = 4;

	body: 1, 1, M, null, eye;

	module load: "./libmodule-cudatest.so";
	user defined: 1, cuda test,
		1,
		N,
		K/N, C/N,
		K/N, C/N,
		K/N, C/N,
		K/N, C/N;
end: elements;

