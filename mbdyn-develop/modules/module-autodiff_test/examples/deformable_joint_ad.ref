reference: ref_id_node1,
	# position
	reference, global, node1x, node1y, node1z,
	# orientation
	reference, global, euler123, node1Phix, node1Phiy, node1Phiz,
	# velocity
	reference, global, node1vx, node1vy, node1vz,
	# angular velocity
	reference, global, node1wx, node1wy, node1wz;

reference: ref_id_node2,
	# position
	reference, global, node2x, node2y, node2z,
	# orientation
	reference, global, euler123, node2Phix, node2Phiy, node2Phiz,
	# velocity
	reference, global, node2vx, node2vy, node2vz,
	# angular velocity
	reference, global, node2wx, node2wy, node2wz;

reference: ref_id_spring1,
        # position
        reference, global, spring1x, spring1y, spring1z,
        # orientation
        reference, global, eye,
							      
        # velocity
        reference, global, null,
        # angular velocity
        reference, global, null;	

reference: ref_id_spring2,
        # position
        reference, global, spring2x, spring2y, spring2z,
        # orientation
        reference, global, eye,
							      
        # velocity
        reference, global, null,
        # angular velocity
        reference, global, null;

reference: ref_id_spring3,
        # position
        reference, global, spring3x, spring3y, spring3z,
        # orientation
        reference, global, eye,
							      
        # velocity
        reference, global, null,
        # angular velocity
        reference, global, null;
